(version 1)
(allow default)
(deny file-ioctl)
(deny generic-issue-extension)
(deny iokit-issue-extension)
(deny iokit-open-service)
(deny iokit-set-properties)
(deny ipc*)
(deny ipc-posix*)
(deny ipc-posix-issue-extension)
(deny ipc-posix-sem*)
(deny ipc-posix-sem-create)
(deny ipc-posix-sem-open)
(deny ipc-posix-sem-post)
(deny ipc-posix-sem-unlink)
(deny ipc-posix-sem-wait)
(deny ipc-posix-shm*)
(deny ipc-posix-shm-read-data)
(deny ipc-posix-shm-write*)
(deny ipc-posix-shm-write-create)
(deny ipc-posix-shm-write-data)
(deny ipc-posix-shm-write-unlink)
(deny ipc-sysv*)
(deny ipc-sysv-msg)
(deny ipc-sysv-sem)
(deny ipc-sysv-shm)
(deny mach-issue-extension)
(deny mach-lookup
	(require-all
		(require-not (extension com.apple.pluginkit.plugin-service))
		(require-not (extension com.apple.webkit.extension.mach))
		(require-not (extension com.apple.app-sandbox.mach))
		(require-any
			(debug-mode)
			(global-name "com.apple.dt.testmanagerd.uiprocess"))))
(deny process-exec*)
(deny process-exec-interpreter)
(deny socket-ioctl)
(deny syscall-unix
	(require-all
		(require-not (c1 05b8 384f 3858))
		(require-any
			(syscall-number SYS_sigaltstack)
			(require-all
				(require-not (syscall-number SYS_openat))
				(require-not (syscall-number SYS_fstatat))
				(require-not (syscall-number SYS_open))
				(require-not (syscall-number 38))
				(require-not (syscall-number SYS_memorystatus_control))
				(require-not (syscall-number SYS_fstatat64))
				(require-not (syscall-number SYS_dup))
				(require-not (syscall-number SYS___mac_syscall))
				(syscall-number SYS_getfsstat64)
				(syscall-number SYS_map_with_linking_np)
				(syscall-number SYS_getfsstat)))))
(deny syscall-mach
	(require-not (cb 05c2 3a68 384d)))
(deny syscall-mig
	(require-not (cc 05c4 3a68 384b)))
(deny system-fcntl
	(require-not (ce 05ca 3a68 3849)))
(deny system-kas-info)
(deny system-mac-syscall
	(mac-syscall-number 67)
	(require-not (mac-syscall-number 2))
	(require-not (mac-policy-name "Sandbox"))
	(require-all
		(mac-syscall-number 1)
		(mac-policy-name "vnguard"))
	(require-all
		(mac-syscall-number 90)
		(mac-policy-name "AMFI")))
(deny system-necp-client-action)
(deny storage-class-map)
